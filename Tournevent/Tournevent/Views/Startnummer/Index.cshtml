@model IEnumerable<IEnumerable<int>>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            @foreach (var i in item)
            {
            <td>
                @Html.ActionLink(i.ToString(), "Details", new { id = i})
            </td>
            }
        </tr>
    }

    </table>